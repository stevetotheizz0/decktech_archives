document.buzzboardform.convertedtext.value = "";
oldtext=document.buzzboardform.convertedtext.value + "<i>"+newtext+"</i>";
document.buzzboardform.convertedtext.value = oldtext;
document.buzzboardform.convertedtext.focus()
}

