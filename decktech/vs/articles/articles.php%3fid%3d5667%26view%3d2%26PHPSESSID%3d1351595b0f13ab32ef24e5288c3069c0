  <area shape="rect" coords="300,2,350,17" href="/vs/resources/buzz.php?PHPSESSID=1351595b0f13ab32ef24e5288c3069c0">
  <area shape="rect" coords="364,2,428,17" href="/vs/resources/index.php?PHPSESSID=1351595b0f13ab32ef24e5288c3069c0">
