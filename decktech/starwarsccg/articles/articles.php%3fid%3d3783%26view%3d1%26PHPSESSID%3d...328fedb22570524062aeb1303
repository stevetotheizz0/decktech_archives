<tr><td width="98%" colspan=2><table width="100%" border="0" cellspacing="0" cellpadding="5" class="cbody">
<tr><td><font class="h1font"> A Player’s Guide to Winning<br></font><br><font class="h2font"> <b>Overall Rating:</b> </font><img src="/starwarsccg/gfx/theme1/star.gif" width="19" height="18" border="0" align="absmiddle"><img src="/starwarsccg/gfx/theme1/star.gif" width="19" height="18" border="0" align="absmiddle"><img src="/starwarsccg/gfx/theme1/star.gif" width="19" height="18" border="0" align="absmiddle"><img src="/starwarsccg/gfx/theme1/star.gif" width="19" height="18" border="0" align="absmiddle"><img src="/starwarsccg/gfx/theme1/star.gif" width="19" height="18" border="0" align="absmiddle"><font size='1'>(4.92)</font><br><font class="h2font"> <b>Author:</b> <A HREF='http://www.decktech.net/starwarsccg/main/mpage.php?member=1836'>Jonathan "jonnychu" Chu</A></font><font class="afont"> <a href='http://www.decktech.net/starwarsccg/main/mpage.php?member=1836&sub=1'> (Subscribe to this member)</a><br><font class="h2font"> <b>Date Submitted:</b> June 14, 2002</font>  <script language="Javascript" src="http://www.decktech.net/global/dynapi/src/dynapi.js"></script>
<script language="Javascript">

DynAPI.setLibraryPath('http://www.decktech.net/global/dynapi/src/lib/');
DynAPI.include('dynapi.api.*');
DynAPI.include('dynapi.event.*');

DynAPI.onLoad=function() { 
	pageX=0;
	pageY=0;
	var d = new EventListener()
	d.onmousemove = function(e) {
          pageX = e.getPageX();
          pageY = e.getPageY();
    }
	DynAPI.document.addEventListener(d);
}        

function RateComment(n){
	if(DynAPI.document.all['form_titlebar']){
		form_titlebar.setVisible(true);
		form_titlebar.moveTo(pageX,pageY)

	}else{
	  form_titlebar = new DynLayer('form_titlebar',pageX,pageY,200,200,'#333333');
	  var html = '<TABLE width="100%" border=1 bordercolor="#333333" cellpading="0" cellspacing="0"><TR><TD><a href="javascript:form_titlebar.setVisible(false)"><IMG SRC="http://www.decktech.net/global/images/x.gif" WIDTH="18" HEIGHT="15" BORDER=0 hspace="2" align="right"></a><font class="h2font">This review was...</font></TD></TR></TABLE>';		
	  form_titlebar.setHTML(html);
	  DynAPI.document.addChild(form_titlebar);

	  reviewform = new DynLayer(null,0,20,200,180,'#000000');

	  var htmlform = '<form name="reviewrate" method="post" action="/starwarsccg/articles/articles.php" onSubmit="return submitRR(this)"><input type="hidden" name="PHPSESSID" value="...328fedb22570524062aeb1303" /><TABLE height="180" width="200" border=1 bordercolor="#333333" cellpading="0" cellspacing="0"><TR><TD><table width="100%" border="0" cellspacing="0" cellpadding="2"><tr><td width="4%"><input type="radio" name="rating" value="4"></td><td width="96%">very helpful</td></tr><tr><td width="4%"><input type="radio" name="rating" value="3" checked></td><td width="96%">somewhat helpful</td></tr><tr><td width="4%"><input type="radio" name="rating" value="2"></td><td width="96%">not much help</td></tr><tr><td width="4%"><input type="radio" name="rating" value="1"></td><td width="96%">completely wrong</td></tr><tr><td width="4%"><input type="checkbox" name="delcom" value=""></td><td width="96%">Delete this review for 250 points.</td></tr><tr><td width="4%">&nbsp;</td><td width="96%"><input type="hidden" name="view" value="1"><input type="hidden" name="deleteid" value=""><input type="hidden" name="id" value="3783"><input type="hidden" name="reviews" value=""><input type="hidden" name="ratereview" value="1"><input type="submit" name="Submit" value="Submit"></td></tr></table></TD></TR></TABLE></form>';
	  reviewform.setHTML(htmlform);
  	  form_titlebar.addChild(reviewform);
	  DragEvent.setDragBoundary(form_titlebar,0,DynAPI.document.getWidth(),10000,0);
	  DragEvent.enableDragEvents(form_titlebar);

      myListener1 = new EventListener(DynAPI.document);
      myListener1.onmousedown=function(e) {
      	e.setBubble(false);
  	  	}
  	  reviewform.addEventListener(myListener1);
	}
  review_id = n;
}

function submitRR(form) {
	if (form.delcom.checked==true)
	{
		input_box=confirm("Click OK to spend 250 Tech Points and delete this review.");
		if (input_box==true)
		{ 
			form.ratereview.value = review_id;
			form.delcom.value = review_id;
			form.deleteid.value = review_id;
			return true;
		}
		else
		{
			// Output when Cancel is clicked
			return false;
		}
	}
	else 
	{
	form.delcom.value = "";
	form.ratereview.value = review_id;
	return true;	
	}
}
