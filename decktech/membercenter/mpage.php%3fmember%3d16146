function adminLoad() {
	if(DynAPI.document.all['form_adminPanel']){
		form_adminPanel.setVisible(true);
	}else{
  form_adminPanel = new DynLayer('form_adminPanel',200,200,400,421,'#000066');
  var html = '<TABLE width="100%" border=1 bordercolor="#000066" cellpading="0" cellspacing="0"><TR><TD><a href="javascript:form_adminPanel.setVisible(false)"><IMG SRC="http://www.decktech.net/global/images/x.gif" WIDTH="18" HEIGHT="15" BORDER=0 hspace="2" align="right"></a><font face="Arial, Helvetica, sans-serif" size="2"><B>Member Admin Panel</font></TD></TR></TABLE>';		
  form_adminPanel.setHTML(html);
  DynAPI.document.addChild(form_adminPanel);


  lp = new LoadPanel();
  lp.setHTML("no file loaded");
	
  lp.setSize(398,400); // height will be overwritten by default
  lp.moveTo(1,20);
  lp.setBgColor('#001F90');
	
  form_adminPanel.addChild(lp);


  DragEvent.setDragBoundary(form_adminPanel,0,DynAPI.document.getWidth(),10000,0);
  DragEvent.enableDragEvents(form_adminPanel);

	adminOpen('admin.php?member=16146');
	}
}

function adminOpen(page) {
lp.setHTML("Loading...");
lp.setURL(page);
}

function submitAdmin() {
//Put all the values in a query string

qstring = "admin.php?member=" + lp.doc.admfrm.member.value;
qstring = qstring + "&delete_decks=" + lp.doc.admfrm.delete_decks.checked;
qstring = qstring + "&delete_treports=" + lp.doc.admfrm.delete_treports.checked;
qstring = qstring + "&delete_articles=" + lp.doc.admfrm.delete_articles.checked;
qstring = qstring + "&delete_reviews=" + lp.doc.admfrm.delete_reviews.checked;
qstring = qstring + "&delete_dmail=" + lp.doc.admfrm.delete_dmail.checked;
qstring = qstring + "&delete_page=" + lp.doc.admfrm.delete_page.checked;
qstring = qstring + "&ban=" + lp.doc.admfrm.ban.checked;
qstring = qstring + "&set_techpoints=" + lp.doc.admfrm.set_techpoints.checked;
qstring = qstring + "&techpoints=" + lp.doc.admfrm.techpoints.value;
qstring = qstring + "&comments=" + lp.doc.admfrm.comments.value;
//alert(qstring);
adminOpen(qstring);
}


