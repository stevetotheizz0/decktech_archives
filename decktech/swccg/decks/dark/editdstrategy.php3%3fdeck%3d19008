                <td height="622" valign="top" colspan="2" bgcolor="#333366"> <table width="100%" border="0" cellpadding="4"><tr bgcolor="#000000"><td><font color="#FFFFFF"><font face="Haettenschweiler, Tahoma, Arial" size="6">AT-AT&#146;s Cometh</font></td></tr></table> 
                  <script language="javascript">
function openCardList(deck)
	{
var DL;
DL = "cardlist.php3?deck=" + deck; 
var SITE = window.open(DL,"cardlist","menubar=0,toolbar=0,location=0,directories=0,status=1,scrollbars=1,width=312,height=400,innerHeight=400,screenx=150,screeny=80,resizable=0,left=150,top=80");
