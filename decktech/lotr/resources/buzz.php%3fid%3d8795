oldtext=document.buzzboardform.comments.value + "<a href="+linkyaddress+" target=x>"+newtext+"</a>";

document.buzzboardform.comments.value = oldtext;

document.buzzboardform.comments.focus()
}

function oldchangetoimage()
{

