<script language="Javascript">

DynAPI.setLibraryPath('http://www.decktech.net/global/dynapi/src/lib/');
DynAPI.include('dynapi.api.*');
DynAPI.include('dynapi.event.*');
DynAPI.include('dynapi.gui.loadlayer.js');

//Variable Declarations
var card_form = new Array();
var card_detail = new Array();
var carddetail = new Array();
var uplayer = 0;

DynAPI.onLoad=function() { 
	pageX=0;
	pageY=0;
	var d = new EventListener()
	d.onmousemove = function(e) {
          pageX = e.getPageX();
          pageY = e.getPageY();
    }
	DynAPI.document.addEventListener(d);
}        


function card_browser(n){
	if(DynAPI.document.all['card_form['+n+']']){
		card_form[n].setVisible(true);
		card_form[n].moveTo(pageX,pageY)

	}else{
	  card_form[n] = new DynLayer('card_form['+n+']',pageX,pageY,250,343,'#000000');
	  var html = '<TABLE width="100%" cellpading="0" cellspacing="0"><TR><TD><a href="javascript:card_form['+ n +'].setVisible(false)"><IMG SRC="http://www.decktech.net/global/images/x.gif" WIDTH="18" HEIGHT="15" BORDER=0 hspace="2" align="right" alt="Close"></a><a href="javascript:cardOpen('+ n +')"><IMG SRC="http://www.decktech.net/global/images/refresh.gif" WIDTH="18" HEIGHT="15" BORDER=0 hspace="2" align="right" alt="Refresh"></a><font class="h2font"><font color="#FFFFFF">Card Browser</font></font></TD></TR></TABLE>';		
	  card_form[n].setHTML(html);
	  DynAPI.document.addChild(card_form[n]);


  carddetail[n] =  new DynLayer( null, 10, 20, 228, 313, "#ffffff")
  carddetail[n].toLoadLayer("cb1.php?card="+n,"<B>Loading...</B>");
  card_form[n].addChild(carddetail[n]);

  myListener1 = new EventListener(DynAPI.document);
  myListener1.onmousedown=function(e) {
      	e.setBubble(false);
  	  	}
  carddetail[n].addEventListener(myListener1);

  DragEvent.setDragBoundary(card_form[n],0,DynAPI.document.getWidth(),10000,0);
  DragEvent.enableDragEvents(card_form[n]);

//  cardOpen(n,n);


	myListenerDrag=new EventListener(card_form[n]) 

	myListenerDrag.ondragstart=function(e){ 
	    o=e.getTarget();
	    o.setZIndex(+(card_form.length+uplayer))
		uplayer = uplayer +1;
	   }

	 card_form[n].addEventListener(myListenerDrag)
	}
  review_id = n;
}

function cardOpen(card) {
carddetail[card].setURL("cb1.php?card="+card);
}


</script><table width=100% border=0 cellpadding=0 cellspacing=0>
