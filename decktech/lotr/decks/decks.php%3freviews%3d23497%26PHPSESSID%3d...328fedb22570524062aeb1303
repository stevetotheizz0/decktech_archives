	  form_titlebar.setHTML(html);
	  DynAPI.document.addChild(form_titlebar);

	  reviewform = new DynLayer(null,0,20,200,180,'#FFFFFF');

	  var htmlform = '<form name="reviewrate" method="post" action="/lotr/decks/decks.php" onSubmit="return submitRR(this)"><input type="hidden" name="PHPSESSID" value="...328fedb22570524062aeb1303" /><TABLE height="180" width="200" border=1 bordercolor="#959988" cellpading="0" cellspacing="0"><TR><TD><table width="100%" border="0" cellspacing="0" cellpadding="2"><tr><td width="4%"><input type="radio" name="rating" value="4"></td><td width="96%">very helpful</td></tr><tr><td width="4%"><input type="radio" name="rating" value="3" checked></td><td width="96%">somewhat helpful</td></tr><tr><td width="4%"><input type="radio" name="rating" value="2"></td><td width="96%">not much help</td></tr><tr><td width="4%"><input type="radio" name="rating" value="1"></td><td width="96%">completely wrong</td></tr><tr><td width="4%"><input type="checkbox" name="delcom" value=""></td><td width="96%">Delete this review for 250 points.</td></tr><tr><td width="4%">&nbsp;</td><td width="96%"><input type="hidden" name="view" value=""><input type="hidden" name="deleteid" value=""><input type="hidden" name="id" value=""><input type="hidden" name="reviews" value="23497"><input type="hidden" name="ratereview" value="1"><input type="submit" name="Submit" value="Submit"></td></tr></table></TD></TR></TABLE></form>';
	  reviewform.setHTML(htmlform);
